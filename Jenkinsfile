node('ci-server'){
    stage('lint code') {
        sh 'env'
    }
    stage('code review') {
        print 'sonarqube code review'
    }
    stage('unit tests') {
        print 'unit tests'
    }
    stage('integration tests') {
        print 'integration tests'
    }
    stage('build application'){
        print 'npm install'
    }
    stage('release'){
        print 'release'
    }
}